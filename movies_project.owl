  <rdf:Description rdf:about="http://semanticweb.org/cleme/projet#tony_kaye">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://semanticweb.org/cleme/projet#Person"/>
